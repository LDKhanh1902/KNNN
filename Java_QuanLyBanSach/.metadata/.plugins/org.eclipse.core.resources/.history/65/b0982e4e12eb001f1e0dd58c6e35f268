package vn.DA_KNNN.View;

import javax.swing.JPanel;

import vn.DA_KNNN.Components.AppHelper;
import vn.DA_KNNN.Components.BackgroundPanel;

import java.awt.GridLayout;

import javax.swing.JFrame;
import javax.swing.JLabel;
import java.awt.BorderLayout;

public class Test extends JPanel {

	private static final long serialVersionUID = 1L;

	/**
	 * Create the panel.
	 */
	public Test() {
		setLayout(new GridLayout(1, 2, 0, 0));
		
		JPanel panelBanner = new BackgroundPanel("/images/add.png");
		add(panelBanner);
		panelBanner.setLayout(new BorderLayout(0, 0));
		
		JPanel panelControl = new JPanel();
		add(panelControl);

	}
	
	public static void main(String[] args) {
		new JFrame().add(new Test()).setVisible(true);
	}

}
