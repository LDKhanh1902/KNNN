package vn.DA_KNNN.View;

import javax.swing.*;

public class Test {
    public static void main(String[] args) {
        // Tạo một JFrame mới
        JFrame f = new JFrame();
        f.setSize(500, 400);

        // Tạo một JLabel với văn bản và biểu tượng (ImageIcon)
        JLabel l = new JLabel("book", new ImageIcon(Test.class.getResource("/images/imgIcon.jpeg")), JLabel.CENTER);
        
        // Thêm JLabel vào JFrame
        f.add(l);
        
        // Đóng chương trình khi cửa sổ đóng
        f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        
        // Hiển thị JFrame
        f.setVisible(true);
    }
}
